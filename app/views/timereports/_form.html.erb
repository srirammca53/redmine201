
<%= form_for(@timereport) do |f| %>
 <%= f.label :user %> : <br>
 <%= f.text_field :user %><br>
  <%= f.label :start_date %> : <br>
 <%#= f.date_select :start_date %> 
     <%= f.text_field :start_date, :id => :popupDatepicker%>
<br>
<%= f.label :end_date %> : <br>
     <%= f.text_field :end_date, :id => :popupDatepicker1 %>
 <br>
  <div class="actions">
    <%= f.submit  "Report"%>
  </div>
<% end %>
 <script>
$(function() {
	$('#popupDatepicker').datepick({dateFormat: 'yyyy-mm-dd'});
	
	$('#popupDatepicker1').datepick({dateFormat: 'yyyy-mm-dd'});
});

</script>
