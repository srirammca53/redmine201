<%= form_for [@task, @task.logs.build] do |f| %>
	Reported Date: <%= f.text_field :report_date%><br>
	Spent Hours: <%= f.text_field :spent_hours%><br>
	Remaining Time: <%= f.text_field :remaining_time%><br>
	Estimated Time: <%= f.text_field :estimated_time%><br>
	User One: <%= f.text_field :user1%><br>
	User Two: <%= f.text_field :user2%><br>
	<%= f.submit %>
<%end%>
