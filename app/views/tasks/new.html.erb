<html>
<p> New Task for <b><%#= @storie.name %></b> </p>
<table border="1" >
<tr>
<%= form_for @task, :url => {:action => :create, :id => @story_id } do |f| %>
 <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :task_type %><br />
    <%= f.text_field :task_type %>
  </div>
  <div class="field">
    <%= f.label :disposition %><br />
    <%= f.text_field :disposition %>
  </div>
  <div class="field">
    <%= f.label :acceptor %><br />
    <%= f.text_field :acceptor %>
  </div>
 <div class="field">
    <%= f.label :estimated_hours %><br />
    <%= f.text_field :estimated_hours %>
  </div>
 <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>


  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
